{
  "name": "socially",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:network": "next dev --turbopack --hostname 0.0.0.0 --port 3000",
    "dev:full": "concurrently \"npm run socket:dev\" \"npm run dev:network\"",
    "socket:dev": "node --watch socket-server.js",
    "socket:build": "tsc socket-server.ts --outDir dist --target es2020 --module commonjs --moduleResolution node --esModuleInterop true --allowSyntheticDefaultImports true --strict true",
    "socket:start": "node dist/socket-server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.12.5",
    "@hookform/resolvers": "^3.10.0",
