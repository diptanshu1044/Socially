{
  "name": "socially",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:network": "next dev --turbopack --hostname 0.0.0.0 --port 3000",
    "dev:full": "concurrently \"npm run socket:dev\" \"npm run dev:network\"",
    "socket:dev": "tsx watch socket-server.ts",
    "socket:build": "tsc socket-server.ts --outDir dist --target es2020 --module commonjs --moduleResolution node --esModuleInterop true --allowSyntheticDefaultImports true --strict true",
    "socket:start": "node dist/socket-server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.8.3",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-sheet": "^1.1.4",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@tanstack/react-query": "^5.62.15",
    "@uploadthing/react": "^7.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "next": "15.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^1.7.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "uploadthing": "^7.5.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "concurrently": "^9.2.0",
    "eslint": "9.18.0",
    "eslint-config-next": "15.2.2",
    "postcss": "8.5.1",
    "prisma": "^6.1.0",
    "tailwindcss": "3.5.0",
    "tsx": "^4.19.2",
    "typescript": "5.7.2"
  }
}
